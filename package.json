{
  "name": "openshift4-nodejs-ts-mysql-ex",
  "version": "0.1.0",
  "description": "Example TypeScript Project for OpenShift 4",
  "main": "./dist/src/main.js",
  "scripts": {
    "start:local": "export $(cat .env | grep -v ^# | xargs) && node ./dist/src/main.js",
    "start": "node ./dist/src/main.js",
    "build": "tsc -p tsconfig.json; mkdir ./dist/src/server/public && cp ./src/server/public/* ./dist/src/server/public;",
    "clean": "find . -name '*.js' -not -path './node_modules/*' ! -name 'gulpfile.js' | xargs rm; find . -name '*.map' -not -path './node_modules/*' | xargs rm; rm -rf dist",
    "test": "./node_modules/mocha/bin/_mocha",
    "watch": "tsc -w -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillin77/openshift4-nodejs-ts-mysql-by-mysql2-ex"
  },
  "homepage": "https://github.com/phillin77/openshift4-nodejs-ts-mysql-by-mysql2-ex#README.md",
  "keywords": [
    "Node",
    "TypeScript"
  ],
  "author": "ISC",
  "license": "ISC",
  "engines": {
    "node": ">=8.10.0"
  },
  "nodemonConfig": {
    "ignore": [
      "logs/*",
      "*db.json"
    ]
  },
  "dependencies": {
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "mocha": "^6.1.4",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.3.0",
    "ramda": "^0.25.0",
    "serve-favicon": "^2.5.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/async": "^2.4.1",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^11.13.5",
    "@types/node-fetch": "^2.3.2",
    "@types/ramda": "^0.25.33",
    "@types/request-promise": "^4.1.42",
    "@types/serve-favicon": "^2.2.30",
    "typescript": "^3.5.3"
  }
}
